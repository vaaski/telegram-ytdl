    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^46.0.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "tsx": "^3.12.6",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.1"
  },
  "dependencies": {
    "@grammyjs/menu": "^1.2.0",
